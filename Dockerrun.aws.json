{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "yaqoobc/docker-project-client",
      "hostname": "clinet",
      "essential": false
    },

    {
      "name": "worker",
      "image": "yaqoobc/docker-project-worker",
      "hostname": "worker",
      "essential": false
    },

    {
      "name": "api",
      "image": "yaqoobc/docker-project-server",
      "hostname": "api",
      "essential": false
    },

    {
      "name": "nginx",
      "image": "yaqoobc/docker-project-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
        "link": ["client", "server"]
    }
  ]
}
